local Webhook = (loadstring(game:HttpGet("https://raw.githubusercontent.com/AmasNot/Real/refs/heads/main/WH")))();
if game.PlaceId == 2753915549 then
	World1 = true;
elseif game.PlaceId == 4442272183 then
	World2 = true;
elseif game.PlaceId == 7449423635 then
	World3 = true;
end;
local SendMirage = false;
spawn(function()
	while wait(0.2) do
		if World2 or World3 then
			if game.Workspace._WorldOrigin.Locations:FindFirstChild("Mirage Island") and (not SendMirage) then
				local Config = {
					URL = "https://discord.com/api/webhooks/1369098529997590578/mtCxpewkI4ixYHPKziXDyknXYR5CCjOH5OC8vQGdzN7U2YXgMENpPpjyRIq-DQRw5utZ",
					Title = "Mirage Island Finder",
					Description = "Make sure it matches the sea listed!",
					Content = {
						{
							Name = "[ðŸš€] Mirage Island Status",
							Value = "Mirage Island (Spawned)",
							Inline = false
						}
					}
				};
				Webhook:Send(Config);
				SendMirage = true;
			end;
		end;
	end;
end);
