function getFullMoon()
	if (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149431" then
		return "Progress 100%";
	elseif (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149052" then
		return "Progress 75%";
	else
		return false;
	end;
end;
local SendFullMoon = false;
spawn(function()
	while wait(0.2) do
		if World3 then
			if getFullMoon() and (not SendFullMoon) then
				local Config = {
					URL = "https://discordapp.com/api/webhooks/1369098604525912206/gR76VA60kdEUuXVRYHINUb53lxxeQeNn0RhNXyfszr04qUaGU1j7cNw8hPeGqTk60wf-",
					Title = "Full Moon Finder",
					Description = "Make sure it matches the sea listed!",
					Content = {
						{
							Name = "[ðŸŒ•] Full Moon",
							Value = "" .. getFullMoon() .. "",
							Inline = false
						}
					}
				};
				Webhook:Send(Config);
				SendFullMoon = true;
			end;
		end;
	end;
end);
